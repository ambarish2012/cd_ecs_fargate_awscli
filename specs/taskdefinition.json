{
    "family": "${FAMILY}",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "${CONTAINER_NAME}",
            "image": "${APP_IMG}:${APP_TAG}",
            "portMappings": [
                {
                    "containerPort": ${CONTAINER_PORT},
                    "hostPort": ${HOST_PORT}
                }
            ],

            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "${CPU}",
    "memory": "${MEMORY}"
}
