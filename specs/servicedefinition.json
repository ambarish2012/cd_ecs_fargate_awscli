{
    "cluster": "${CLUSTER_NAME}",
    "serviceName": "${SERVICE_NAME}",
    "taskDefinition": "${FAMILY}:{REVISION}",
    "desiredCount": 1,
    "launchType": "FARGATE",
    "networkConfiguration": {
